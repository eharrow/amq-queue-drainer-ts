{
	"name": "amq-queue-drainer",
	"version": "1.2.0",
	"description": "CLI to drain or fill a Rabbit AMQ queue.",
	"keywords": [
		"rabbit",
		"rabbitmq",
		"cli",
		"amqp",
		"drainer",
		"utilities"
	],
	"homepage": "https://github.com/eharrow/amq-queue-drainer/blob/master/README.md",
	"main": "dist/src/index.js",
	"preferGlobal": true,
	"directories": {
		"bin": "./bin"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eharrow/amq-queue-drainer.git"
	},
	"dependencies": {
		"@types/prompts": "^2.0.5",
		"amqplib": "^0.10.3",
		"chalk": "^5.2.0",
		"cli-spinner": "^0.2.10",
		"commander": "^11.0.0",
		"figlet": "^1.3.0",
		"i18n": "^0.15.1",
		"node-emoji": "^2.1.0",
		"prompts": "^2.3.2",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.1",
		"@types/cli-spinner": "^0.2.0",
		"@types/figlet": "^1.2.0",
		"@types/i18n": "^0.13.6",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.3.0",
		"@types/node-emoji": "^1.8.1",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.59.11",
		"eslint": "^8.43.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"jest-extended": "^4.0.0",
		"prettier": "^2.0.4",
		"rabbitmq-eventemitter-mock": "^1.2.0",
		"supertest": "^6.3.3",
		"testcontainers": "^9.9.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3"
	},
	"scripts": {
		"lint": "eslint src/* --fix",
		"format": "prettier --write \"src/*.ts\"",
		"test": "jest",
		"intTest": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc"
	},
	"lint-staged": {
		"linters": {
			"src/*.ts": [
				"prettier --write",
				"git add"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"engines": {
		"node": ">=8"
	},
	"author": "ewan@harrow.org",
	"license": "ISC"
}
