{
	"name": "amq-queue-drainer",
	"version": "1.2.0",
	"description": "CLI to drain or fill a Rabbit AMQ queue.",
	"keywords": [
		"rabbit",
		"rabbitmq",
		"cli",
		"amqp",
		"drainer",
		"utilities"
	],
	"homepage": "https://github.com/eharrow/amq-queue-drainer/blob/master/README.md",
	"main": "dist/src/index.js",
	"preferGlobal": true,
	"directories": {
		"bin": "./bin"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eharrow/amq-queue-drainer-ts.git"
	},
	"dependencies": {
		"@types/prompts": "^2.4.9",
		"amqplib": "^0.10.9",
		"chalk": "^4.1.2",
		"cli-spinner": "^0.2.10",
		"commander": "^14.0.1",
		"figlet": "^1.9.3",
		"i18n": "^0.15.2",
		"node-emoji": "^2.2.0",
		"prompts": "^2.3.2",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.7",
		"@types/cli-spinner": "^0.2.3",
		"@types/figlet": "^1.7.0",
		"@types/i18n": "^0.13.12",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.9.1",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"eslint": "^9.38.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"jest": "^30.2.0",
		"jest-extended": "^6.0.0",
		"prettier": "^3.6.2",
		"rabbitmq-eventemitter-mock": "^1.2.0",
		"supertest": "^7.1.4",
		"testcontainers": "^11.7.1",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	},
	"scripts": {
		"lint": "eslint src/* --fix",
		"format": "prettier --write \"src/*.ts\"",
		"test": "jest --passWithNoTests",
		"intTest": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc"
	},
	"lint-staged": {
		"linters": {
			"src/*.ts": [
				"prettier --write",
				"git add"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"engines": {
		"node": ">=16"
	},
	"author": "ewan@harrow.org",
	"license": "ISC"
}
