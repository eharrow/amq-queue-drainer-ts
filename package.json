{
	"name": "amq-queue-drainer",
	"version": "1.2.0",
	"description": "CLI to drain or fill a Rabbit AMQ queue.",
	"keywords": [
		"rabbit",
		"rabbitmq",
		"cli",
		"amqp",
		"drainer",
		"utilities"
	],
	"homepage": "https://github.com/eharrow/amq-queue-drainer/blob/master/README.md",
	"main": "dist/src/index.js",
	"preferGlobal": true,
	"directories": {
		"bin": "./bin"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eharrow/amq-queue-drainer.git"
	},
	"dependencies": {
		"@types/prompts": "^2.0.5",
		"amqplib": "^0.10.3",
		"chalk": "^4.0.0",
		"cli-spinner": "^0.2.10",
		"commander": "^5.0.0",
		"figlet": "^1.3.0",
		"i18n": "^0.9.0",
		"node-emoji": "^1.10.0",
		"prompts": "^2.3.2",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.1",
		"@types/cli-spinner": "^0.2.0",
		"@types/figlet": "^1.2.0",
		"@types/i18n": "^0.8.6",
		"@types/node": "^13.13.0",
		"@types/node-emoji": "^1.8.1",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"eslint": "^6.8.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.3",
		"prettier": "^2.0.4",
		"rabbitmq-eventemitter-mock": "^1.2.0",
		"typescript": "^3.8.3"
	},
	"scripts": {
		"lint": "eslint src/* --fix",
		"format": "prettier --write \"src/*.ts\"",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc"
	},
	"lint-staged": {
		"linters": {
			"src/*.ts": [
				"prettier --write",
				"git add"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"engines": {
		"node": ">=8"
	},
	"author": "ewan@harrow.org",
	"license": "ISC"
}
